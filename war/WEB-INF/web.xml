<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GAE_Application1</servlet-name>
		<servlet-class>com.haiduong.gaeapplication.GAE_Application1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GAE_Application1</servlet-name>
		<url-pattern>/gae_application1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GAE_Application1</servlet-name>
		<url-pattern>/demo</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>sign</servlet-name>
	<servlet-class>com.haiduong.gaeapplication.SignGuestbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>sign</servlet-name>
	<url-pattern>/sign</url-pattern>
	</servlet-mapping> 
	
	<servlet>
	<servlet-name>command</servlet-name>
	<servlet-class>com.haiduong.gaeapplication.Transerver</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>command</servlet-name>
	<url-pattern>/command</url-pattern>
	</servlet-mapping> 
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
